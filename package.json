{
  "name": "vue-webpack-latest-template",
  "version": "0.1.0",
  "description": "A Vuejs template using Webpack 4",
  "author": "Kirichenko Sergei",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "prod": "webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "test": "npm run unit && npm run e2e",
    "unit": "jest",
    "e2e": "node test/e2e/runner.js",
    "server": "node server.js"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.25.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.15",
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-plugin-universal-import": "^1.5.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "csso": "^3.5.0",
    "csso-webpack-plugin": "^1.0.0-beta.12",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-optipng": "^5.2.1",
    "imagemin-pngquant": "^5.1.0",
    "imagemin-svgo": "^6.0.0",
    "imagemin-webp": "^4.1.0",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "modularscale-sass": "^3.0.5",
    "nightwatch": "^0.9.21",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "rename-webpack-plugin": "^1.0.1",
    "requires-port": "^1.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^3.7.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-analytics": "^5.10.4",
    "vue-jest": "^2.5.0",
    "vue-loader": "^15.0.9",
    "vue-server-renderer": "^2.5.16",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vue-tippy": "^2.0.11",
    "webpack": "^4.7.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-monitor": "^1.0.14",
    "webpack-pwa-manifest": "^3.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/**/*.vue",
      "src/pages/**/*.vue",
      "src/state/**/*.js",
      "src/helpers/**/*.js",
      "src/utils/**/*.js"
    ],
    "testMatch": [
      "**/src/components/**/?(*.)(spec|test).js",
      "**/src/pages/**/?(*.)(spec|test).js",
      "**/src/state/**/?(*.)(spec|test).js",
      "**/src/helpers/**/?(*.)(spec|test).js",
      "**/src/utils/**/?(*.)(spec|test).js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": ".jest_coverage",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "last 2 firefox version",
    "last 2 chrome version",
    "last 1 safari version",
    "not ie <= 11",
    "not op_mini all"
  ]
}
